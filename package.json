    "babel-plugin-transform-react-jsx": "^6.24.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "jest-enzyme": "^7.1.2"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-react-jsx"
